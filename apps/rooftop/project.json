{
  "root": "apps/rooftop",
  "sourceRoot": "apps/rooftop/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/web:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/rooftop",
        "index": "apps/rooftop/src/index.html",
        "main": "apps/rooftop/src/main.tsx",
        "polyfills": "apps/rooftop/src/polyfills.ts",
        "tsConfig": "apps/rooftop/tsconfig.app.json",
        "assets": [
          "apps/rooftop/src/assets"
        ],
        "styles": [],
        "scripts": [],
        "webpackConfig": "webpack.config.js"
      },
      "configurations": {
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/rooftop/src/environments/environment.ts",
              "with": "apps/rooftop/src/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "extractCss": true,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "options": {
        "buildTarget": "rooftop:build",
        "hmr": true,
        "browserTarget": "surfboard-api:build:development",
        "proxyConfig": "apps/rooftop/proxy.config.json"
      },
      "configurations": {
        "production": {
          "buildTarget": "rooftop:build:production",
          "hmr": false
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/rooftop/**/*.{ts,tsx,js,jsx}"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/rooftop"
      ],
      "options": {
        "jestConfig": "apps/rooftop/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
