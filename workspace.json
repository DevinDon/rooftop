{
  "version": 2,
  "projects": {
    "components": {
      "root": "libs/components",
      "sourceRoot": "libs/components/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/libs/components",
            "tsConfig": "libs/components/tsconfig.lib.json",
            "project": "libs/components/package.json",
            "entryFile": "libs/components/src/index.ts",
            "external": [
              "react/jsx-runtime"
            ],
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "libs/components/README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "libs/components/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/components"
          ],
          "options": {
            "jestConfig": "libs/components/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "libs/components/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/components",
            "config": {
              "configFolder": "libs/components/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "rooftop": {
      "root": "apps/rooftop",
      "sourceRoot": "apps/rooftop/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/rooftop",
            "index": "apps/rooftop/src/index.html",
            "main": "apps/rooftop/src/main.tsx",
            "polyfills": "apps/rooftop/src/polyfills.ts",
            "tsConfig": "apps/rooftop/tsconfig.app.json",
            "assets": [
              "apps/rooftop/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/rooftop/src/environments/environment.ts",
                  "with": "apps/rooftop/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "rooftop:build",
            "hmr": true,
            "browserTarget": "surfboard-api:build:development",
            "proxyConfig": "apps/rooftop/proxy.config.json"
          },
          "configurations": {
            "production": {
              "buildTarget": "rooftop:build:production",
              "hmr": false
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/rooftop/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/rooftop"
          ],
          "options": {
            "jestConfig": "apps/rooftop/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "rooftop-e2e": {
      "root": "apps/rooftop-e2e",
      "sourceRoot": "apps/rooftop-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/rooftop-e2e/cypress.json",
            "devServerTarget": "rooftop:serve",
            "tsConfig": "apps/rooftop-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "rooftop:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/rooftop-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "surfboard-api": {
      "root": "apps/surfboard-api",
      "projectType": "application",
      "sourceRoot": "apps/surfboard-api/src",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/surfboard-api",
            "main": "apps/surfboard-api/src/main.ts",
            "tsConfig": "apps/surfboard-api/tsconfig.app.json",
            "assets": [
              "apps/surfboard-api/src/assets"
            ]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/surfboard-api/src/environments/environment.ts",
                  "with": "apps/surfboard-api/src/environments/environment.prod.ts"
                }
              ],
              "externalDependencies": "none"
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "surfboard-api:build"
          }
        },
        "mongo": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "docker run --name rester-mongodb-standalone -e MONGO_INITDB_ROOT_USERNAME=rester-dev -e MONGO_INITDB_ROOT_PASSWORD=rester-dev -p 27017:27017 -v rester-mongodb-standalone:/data/db -d mongo:4"
              }
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/surfboard-api/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/surfboard-api"
          ],
          "options": {
            "jestConfig": "apps/surfboard-api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@rester/nx"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "rooftop"
}
