{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "prisma",
  "type": "object",
  "properties": {
    "command": {
      "type": "string",
      "description": "Prisma command",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    },
    "project": {
      "type": "string",
      "description": "Prisma project name",
      "$default": {
        "$source": "argv",
        "index": 1
      }
    }
  },
  "required": [
    "command"
  ]
}
